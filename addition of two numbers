global _start

section .data

                sum: db 0

section .text

                _start:

                    mov ax,8
                    mov cx,1
                    add ax,cx
                    add ax,'o'


                     mov[sum],ax


                      mov edx,1
                      mov ecx,sum
                      mov eax,4
                      mov ebx,1
                      int 0x80


                      mov eax,1
                      xor ebx,ebx
                      int 0x80
